{
    "name": "stixx/omnipay-utrust",
    "description": "Utrust Payments support for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "utrust"
    ],
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "omnipay/common": "^3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "omnipay/tests": "^4.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "rregeer/phpunit-coverage-check": "^0.3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jelle van Oosterbosch",
            "email": "info@celer-it.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Utrust\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Utrust\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
